<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!-- 
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="1" Customer="alps"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Customer="alps"/>  
	
	<!-- fly  -->
	<EccEntry Ecc="112" Category="0" Condition="1" Customer="fly"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Customer="fly"/>
    <EccEntry Ecc="000" Category="0" Condition="0" Customer="fly"/>
	<EccEntry Ecc="101" Category="0" Condition="0" Customer="fly"/>
	<EccEntry Ecc="102" Category="0" Condition="0" Customer="fly"/>
	
	<!-- blu  -->
	<EccEntry Ecc="112" Category="0" Condition="1" Customer="blu"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Customer="blu"/> 
	
	
	<!-- xolo  -->
	<EccEntry Ecc="112" Category="0" Condition="1" Customer="XOLO"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Customer="XOLO"/>  

	<!-- RGK  -->
    <EccEntry Ecc="112" Category="0" Condition="1" Customer="RGK"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Customer="RGK"/>    	
</EccTable>
